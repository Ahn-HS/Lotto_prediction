<Window x:Class="FilterCom.MainWindow"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
                WindowState="Maximized"
                Title="MainWindow" Height="1040" Width="1900" Loaded="onLoade">
    <Window.Resources>


        <ImageBrush x:Key="MyResource" ImageSource="uncheck.png" />
        <ImageBrush x:Key="MyResource2" ImageSource="checkbox.png" />

        <SolidColorBrush x:Key="BtncheckBag" Color="#C6E0B4" />
        <SolidColorBrush x:Key="GroupcheckBag" Color="#FFD599" />
    </Window.Resources>
    
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <telerik:RadMenu Grid.Row="0" x:Name="radMenu" ClickToOpen="True" ItemClick="radMenu_ItemClick" Visibility="Collapsed"  >
            <telerik:RadMenuItem Header="file">
                <telerik:RadMenuItem Header="닫기"/>
            </telerik:RadMenuItem>
            <telerik:RadMenuItem Header="데이터">
                <telerik:RadMenuItem Header="당첨 데이터 일괄 등록" Tag="2_1"/>
                
            </telerik:RadMenuItem>
            
            
        </telerik:RadMenu>
        <telerik:RadBusyIndicator Grid.Row="1" Name="BusyIndicateView" BusyContent="Loading.." IsBusy="False"
				IsIndeterminate="True" >

            <telerik:RadDocking  Grid.Row="1"  BorderThickness="0" Padding="0">
                <telerik:RadDocking.DocumentHost>
                    <telerik:RadSplitContainer >
                        <telerik:RadPaneGroup x:Name="DocumentGroup">
                            <telerik:RadPane x:Name="LayoutXml" Header="파일그룹"  CanUserClose="False"
								    telerik:RadDocking.SerializationTag="LayoutXml"
								    telerik:RadDocking.FloatingSize="450, 400">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"></RowDefinition>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <telerik:RadGridView Name="grid_filegroup" Grid.Row="0"
                                                         
                                                     ShowGroupPanel="False" 
					                                AutoGenerateColumns="False"
					                                CanUserFreezeColumns="False"
                                                     ItemsSource="{Binding FileDatas, Mode=TwoWay}"
                                                     IsFilteringAllowed="False"
                                                     CanUserSortColumns="False"
                                                     CellEditEnded="grid_filegroup_CellEditEnded"
                                                     RowIndicatorVisibility="Collapsed"
                                >
                                        <telerik:RadGridView.RowStyle>
                                            <Style TargetType="telerik:GridViewRow">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding IsChecked}" Value="true">
                                                        <Setter Property="Background" Value="#C6E0B4"></Setter>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </telerik:RadGridView.RowStyle>
                                        <telerik:RadGridView.Columns>
                                            <telerik:GridViewDataColumn >

                                                <telerik:GridViewDataColumn.Header>
                                                    <telerik:RadButton Click="Btn_Filegroup_checked"   Height="20" Width="20" />
                                                </telerik:GridViewDataColumn.Header>
                                                
                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        
                                                        <telerik:RadButton Tag="{Binding}" Click="BtnGrid1Click" Height="20" Width="20" >
                                                            <telerik:RadButton.Style>
                                                                <Style TargetType="telerik:RadButton">
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding IsChecked}" Value="true">
                                                                            <Setter Property="Background" Value="{StaticResource MyResource2}" />
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding IsChecked}" Value="false">
                                                                            <Setter Property="Background" Value="{StaticResource MyResource}" />
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </telerik:RadButton.Style>
                                                        </telerik:RadButton>
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>
                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="" DataMemberBinding="{Binding Idx}" Width="50" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn Header="파일이름" DataMemberBinding="{Binding FileName}" Width="150" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn Header="그룹개수"  DataMemberBinding="{Binding RowCount}" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn   DataMemberBinding="{Binding NoMin}" TextAlignment="Right">
                                                <telerik:GridViewDataColumn.Header>
                                                    <telerik:RadButton Content="최소" Click="Btn_filegroup_all_min"  />
                                                </telerik:GridViewDataColumn.Header>
                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding NoMin, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>
                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="~" DataMemberBinding="{Binding Devidedstr}" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn  DataMemberBinding="{Binding NoMax}" TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>
                                                    <telerik:RadButton Content="최대" Click="Btn_filegroup_all_max"  />
                                                </telerik:GridViewDataColumn.Header>
                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding NoMax, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>
                                            </telerik:GridViewDataColumn>

                                            <telerik:GridViewDataColumn DataMemberBinding="{Binding OrStart}" TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>

                                                    <telerik:RadButton Content="부분(OR)" Click="Btn_filegroup_or_start"  />
                                                </telerik:GridViewDataColumn.Header>

                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding OrStart, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>

                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="~" DataMemberBinding="{Binding Devidedstr}" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn DataMemberBinding="{Binding OrEnd}" TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>

                                                    <telerik:RadButton Content="부분(OR)" Click="Btn_filegroup_or_end"  />
                                                </telerik:GridViewDataColumn.Header>

                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding OrEnd, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>

                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="비고"  DataMemberBinding="{Binding Dscription}"  IsReadOnly="True" />

                                        </telerik:RadGridView.Columns>
                                    </telerik:RadGridView>
                                    <Grid Grid.Row="1">
                                        <WrapPanel>
                                            <telerik:RadRadioButton Name="btn_filegroup1" Content="파일그룹1" Width="70" Tag="1" Click="RBTN_FileGroup_Click" IsChecked="True" />
                                            <telerik:RadRadioButton Name="btn_filegroup2" Content="파일그룹2" Width="70" Tag="2" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup3" Content="파일그룹3" Width="70" Tag="3" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup4" Content="파일그룹4" Width="70" Tag="4" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup5" Content="파일그룹5" Width="70" Tag="5" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup6" Content="파일그룹6" Width="70" Tag="6" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup7" Content="파일그룹7" Width="70" Tag="7" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup8" Content="파일그룹8" Width="70" Tag="8" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup9" Content="파일그룹9" Width="70" Tag="9" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup10" Content="파일그룹10" Width="70" Tag="10" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup11" Content="파일그룹11" Width="70" Tag="11" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup12" Content="파일그룹12" Width="70" Tag="12" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup13" Content="파일그룹13" Width="70" Tag="13" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup14" Content="파일그룹14" Width="70" Tag="14" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup15" Content="파일그룹15" Width="70" Tag="15" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup16" Content="파일그룹16" Width="70" Tag="16" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup17" Content="파일그룹17" Width="70" Tag="17" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup18" Content="파일그룹18" Width="70" Tag="18" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup19" Content="파일그룹19" Width="70" Tag="19" Click="RBTN_FileGroup_Click" />
                                            <telerik:RadRadioButton Name="btn_filegroup20" Content="파일그룹20" Width="70" Tag="20" Click="RBTN_FileGroup_Click" />
                                        </WrapPanel>
                                    </Grid>
                                </Grid>

                            </telerik:RadPane>
                        </telerik:RadPaneGroup>

                        <telerik:RadPaneGroup x:Name="DocumentGroup2">
                            <telerik:RadDocumentPane Header="랜덤숫자" CanUserClose="False"
                                                     
								    telerik:RadDocking.SerializationTag="DocumentPane">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*"></RowDefinition>
                                        <RowDefinition Height="Auto"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <telerik:RadGridView Grid.Row="0" Name="GridRandom" ShowGroupPanel="False" 
					                                AutoGenerateColumns="False"
					                                CanUserFreezeColumns="False"
                                                     IsFilteringAllowed="False"
                                                     CanUserSortColumns="False"
                                                     RowIndicatorVisibility="Collapsed"
                                                     ItemsSource="{Binding FileGroupItems, Mode=TwoWay}"
                                                         SelectionChanged="GridRandom_SelectChange"
                                                         >

                                        <telerik:RadGridView.RowStyle>
                                            <Style TargetType="telerik:GridViewRow">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding R_IsCheck}" Value="true">
                                                        <Setter Property="Background" Value="#C6E0B4"></Setter>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </telerik:RadGridView.RowStyle>
                                        <telerik:RadGridView.Columns>
                                            <telerik:GridViewDataColumn Header="" >
                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <Label Tag="{Binding}" Content="{Binding Idx, Mode=OneWay}" >
                                                            <Label.Style>
                                                                <Style TargetType="Label">
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding IsGroupCheck}" Value="true">
                                                                            <Setter Property="Background" Value="{StaticResource GroupcheckBag}" />
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </Label.Style>
                                                        </Label>
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>
                                                
                                            </telerik:GridViewDataColumn>

                                            <telerik:GridViewDataColumn >

                                                <telerik:GridViewDataColumn.Header>
                                                    <telerik:RadButton Click="Btn_random_checked" Height="20" Width="20"  />
                                                </telerik:GridViewDataColumn.Header>

                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>

                                                        <telerik:RadButton Tag="{Binding}" Click="BtnGrid2Click" Height="20" Width="20">
                                                            <telerik:RadButton.Style>
                                                                <Style TargetType="telerik:RadButton">
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding R_IsCheck}" Value="true">
                                                                          <Setter Property="Background" Value="{StaticResource MyResource2}" />
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding R_IsCheck}" Value="false">
                                                                          <Setter Property="Background" Value="{StaticResource MyResource}" />
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </telerik:RadButton.Style>
                                                        </telerik:RadButton>
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>
                                            </telerik:GridViewDataColumn>
                                            
                                            <telerik:GridViewDataColumn DataMemberBinding="{Binding R_NumStart, Mode=TwoWay}"  TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>

                                                    <telerik:RadButton Content="숫자범위" Click="Btn_random_no_start"  />
                                                </telerik:GridViewDataColumn.Header>

                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding R_NumStart, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" Maximum="{Binding R_NumMax}"  IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>

                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="~" IsReadOnly="True" />
                                            <telerik:GridViewDataColumn  DataMemberBinding="{Binding R_NumEnd, Mode=TwoWay}"  TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>

                                                    <telerik:RadButton Content="숫자범위" Click="Btn_random_no_end"  />
                                                </telerik:GridViewDataColumn.Header>

                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding R_NumEnd, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" Maximum="{Binding R_NumMax}" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>

                                            </telerik:GridViewDataColumn>

                                            <telerik:GridViewDataColumn  DataMemberBinding="{Binding R_CreateCnt, Mode=TwoWay}"  TextAlignment="Right"  >
                                                <telerik:GridViewDataColumn.Header>
                                                    <telerik:RadButton Content="생성개수" />
                                                </telerik:GridViewDataColumn.Header>
                                                <telerik:GridViewDataColumn.CellEditTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadNumericUpDown  Value="{Binding R_CreateCnt, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" Maximum="{Binding R_NumMax}" IsInteger="True" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellEditTemplate>
                                            </telerik:GridViewDataColumn>

                                            <telerik:GridViewDataColumn Header="그룹에자동체크" >

                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadButton Tag="{Binding}" Click="BtnGrid2Click_AutoCheck" >
                                                            <telerik:RadButton.Style>
                                                                <Style TargetType="telerik:RadButton">
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding R_AutoCheck}" Value="true">
                                                                            <Setter Property="Content" Value="자동">
                                                                            </Setter>
                                                                            <Setter Property="Background" Value="{StaticResource BtncheckBag}" />
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding R_AutoCheck}" Value="false">
                                                                            <Setter Property="Content" Value="미자동"></Setter>
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </telerik:RadButton.Style>
                                                        </telerik:RadButton>
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>

                                            </telerik:GridViewDataColumn>

                                            <telerik:GridViewDataColumn Header="균형랜덤" >
                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <telerik:RadButton Tag="{Binding}" Click="BtnGrid2Click_BalanceCheck" >
                                                            <telerik:RadButton.Style>
                                                                <Style TargetType="telerik:RadButton">
                                                                    <Style.Triggers>
                                                                        <DataTrigger Binding="{Binding R_BalanceCheck}" Value="true">
                                                                            <Setter Property="Content" Value="균형">
                                                                            </Setter>
                                                                            <Setter Property="Background" Value="{StaticResource BtncheckBag}" />
                                                                        </DataTrigger>
                                                                        <DataTrigger Binding="{Binding R_BalanceCheck}" Value="false">
                                                                            <Setter Property="Content" Value="비균형"></Setter>
                                                                        </DataTrigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </telerik:RadButton.Style>
                                                        </telerik:RadButton>
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>
                                            </telerik:GridViewDataColumn>
                                            <telerik:GridViewDataColumn Header="생성" >
                                                <telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <!-- Click="BtnRandomItemCreate" Command="{Binding Path=RadGridViewCommandsDelete}"  -->
                                                        
                                                        <telerik:RadButton Content="생성" Click="BtnRandomItemCreate" Tag="{Binding}" />
                                                    </DataTemplate>
                                                </telerik:GridViewDataColumn.CellTemplate>
                                            </telerik:GridViewDataColumn>

                                        </telerik:RadGridView.Columns>
                                    </telerik:RadGridView>

                                </Grid>
                            </telerik:RadDocumentPane>
                        </telerik:RadPaneGroup>
                    </telerik:RadSplitContainer>

                </telerik:RadDocking.DocumentHost>



                <telerik:RadSplitContainer InitialPosition="DockedRight"  MinWidth="400">
                    <telerik:RadPaneGroup >
                        <telerik:RadPane Header="필터 박스"  CanUserClose="False"
							telerik:RadDocking.SerializationTag="PaneRight1">
                            <Grid >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="60" />
                                    <RowDefinition Height="60" />
                                    <RowDefinition Height="60" />
                                </Grid.RowDefinitions>
                                <telerik:GroupBox Grid.Row="0" Header="필터적용">
                                    <Grid >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"></RowDefinition>
                                            <RowDefinition Height="25"></RowDefinition>
                                        </Grid.RowDefinitions>
                                        <telerik:RadGridView Grid.Row="0" Name="GridFilter"
                                                     ShowGroupPanel="False" 
					                                AutoGenerateColumns="False"
					                                CanUserFreezeColumns="False"
                                                     IsFilteringAllowed="False"
                                                     CanUserSortColumns="False"
                                                     RowIndicatorVisibility="Collapsed"
                                                     ItemsSource="{Binding FileGroupItems, Mode=TwoWay}"
                                                             
                                                         SelectionChanged="GridFilter_SelectChange"
                                                             >
                                            <telerik:RadGridView.RowStyle>
                                                <Style TargetType="telerik:GridViewRow">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding F_IsCheck}" Value="true">
                                                            <Setter Property="Background" Value="#C6E0B4"></Setter>
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </telerik:RadGridView.RowStyle>
                                            <telerik:RadGridView.Columns>
                                                <telerik:GridViewDataColumn Header="" >
                                                    <telerik:GridViewDataColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <Label Tag="{Binding}" Content="{Binding Idx, Mode=OneWay}" >
                                                                <Label.Style>
                                                                    <Style TargetType="Label">
                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding IsGroupCheck}" Value="true">
                                                                                <Setter Property="Background" Value="{StaticResource GroupcheckBag}" />
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </Label.Style>
                                                            </Label>
                                                        </DataTemplate>
                                                    </telerik:GridViewDataColumn.CellTemplate>

                                                </telerik:GridViewDataColumn>
                                                <telerik:GridViewDataColumn Header="파일그룹" DataMemberBinding="{Binding FileGroupName, Mode=TwoWay}"/>

                                                <telerik:GridViewDataColumn >

                                                    <telerik:GridViewDataColumn.Header>
                                                        <telerik:RadButton Click="Btn_Filter_checked" Height="20" Width="20"  />
                                                    </telerik:GridViewDataColumn.Header>

                                                    <telerik:GridViewDataColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <telerik:RadButton Tag="{Binding}" Click="BtnGrid3Click" Height="20" Width="20" >

                                                                <telerik:RadButton.Style>
                                                                    <Style TargetType="telerik:RadButton">
                                                                        <Style.Triggers>
                                                                            <DataTrigger Binding="{Binding F_IsCheck}" Value="true">
                                                                                <Setter Property="Background" Value="{StaticResource MyResource2}" />
                                                                            </DataTrigger>
                                                                            <DataTrigger Binding="{Binding F_IsCheck}" Value="false">
                                                                                <Setter Property="Background" Value="{StaticResource MyResource}" />
                                                                            </DataTrigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </telerik:RadButton.Style>
                                                            </telerik:RadButton>
                                                        </DataTemplate>
                                                    </telerik:GridViewDataColumn.CellTemplate>
                                                </telerik:GridViewDataColumn>
                                                
                                                <telerik:GridViewDataColumn  DataMemberBinding="{Binding F_ORStart, Mode=TwoWay}" TextAlignment="Right"  >
                                                    <telerik:GridViewDataColumn.Header>

                                                        <telerik:RadButton Content="부분(OR)" Click="Btn_filter_or_start"  />
                                                    </telerik:GridViewDataColumn.Header>

                                                    <telerik:GridViewDataColumn.CellEditTemplate>
                                                        <DataTemplate>
                                                            <telerik:RadNumericUpDown  Value="{Binding F_ORStart, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" Maximum="{Binding R_NumMax}"  IsInteger="True" />
                                                        </DataTemplate>
                                                    </telerik:GridViewDataColumn.CellEditTemplate>

                                                </telerik:GridViewDataColumn>
                                                <telerik:GridViewDataColumn Header="~" />
                                                <telerik:GridViewDataColumn  DataMemberBinding="{Binding F_OREnd, Mode=TwoWay}" TextAlignment="Right"  >
                                                    <telerik:GridViewDataColumn.Header>

                                                        <telerik:RadButton Content="부분(OR)" Click="Btn_filter_or_end"  />
                                                    </telerik:GridViewDataColumn.Header>

                                                    <telerik:GridViewDataColumn.CellEditTemplate>
                                                        <DataTemplate>
                                                            <telerik:RadNumericUpDown  Value="{Binding F_OREnd, Mode=TwoWay, UpdateSourceTrigger=Explicit}" Minimum="0" Maximum="{Binding R_NumMax}"  IsInteger="True" />
                                                        </DataTemplate>
                                                    </telerik:GridViewDataColumn.CellEditTemplate>

                                                </telerik:GridViewDataColumn>

                                            </telerik:RadGridView.Columns>
                                        </telerik:RadGridView>
                                        <TextBlock Grid.Row="1" Text="체크된 파일 그룹표에서 몇 개의 파일 그룹표를 만족할 것인가?" Foreground="Blue" />
                                    </Grid>
                                </telerik:GroupBox>

                                <Grid Grid.Row="1"  >
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition  />
                                        <ColumnDefinition Width="100" />
                                    </Grid.ColumnDefinitions>
                                    <telerik:GroupBox Grid.Column="0"  Header="부분(OR)" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" >
                                        <StackPanel Orientation="Horizontal">
                                            <telerik:RadNumericUpDown Name="TotalORStart" Width="40" MaxWidth="40" MinWidth="40" Maximum="0" NumberDecimalDigits="0" Minimum="0" ValueFormat="Numeric" Value="0" />
                                            <TextBlock Text="~" />
                                            <telerik:RadNumericUpDown Name="TotalOREnd" Width="40" MaxWidth="40" MinWidth="40" Maximum="0" NumberDecimalDigits="0" Minimum="0" ValueFormat="Numeric" Value="0" />
                                            <StackPanel Orientation="Vertical" >
