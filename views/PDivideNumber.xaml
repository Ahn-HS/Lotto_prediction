<Window x:Class="FilterCom.views.PDivideNumber"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilterCom.views"
        mc:Ignorable="d"
        Title="PDivideNumber" Height="250" Width="800">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition Height="40" />
            <RowDefinition Height="*" />

        </Grid.RowDefinitions>
        <TextBlock Text="나누어지는 숫자 확인" FontSize="20" FontWeight="Bold" Foreground="Blue" TextAlignment="Center" />
        <TextBox Name="lbl_Nums" Grid.Row="1" Margin="8" FontSize="16" FontWeight="Bold" TextWrapping="Wrap" AcceptsReturn="True" Height="auto" IsReadOnly="True"  />
    </Grid>
</Window>
